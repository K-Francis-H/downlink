{
	"manifest_version": 2,
	"name": "downlink",
	"version": "0.1",
	"description": "download websites recursively from the current page",
	"icons":{
		"48": "TODO, put image path here"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"permissions":[
		"contextMenus",
		"downloads"
	],
	"background": {
	  	"scripts": ["background.js"]
	}
}
